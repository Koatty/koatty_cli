{
  "name": "test_config_fallback",
  "version": "1.0.0",
  "description": "application created by koatty",
  "main": "./dist/App.js",
  "scripts": {
    "build": "npm run clear && tsc && npx copyfiles -u 1 ./src/**/*.json ./src/**/*.proto dist/",
    "build:images": "npm run build && docker build -t koatty/test_config_fallback:latest -f deploy/Dockerfile .",
    "clear": "npx del-cli --force dist",
    "dev": "nodemon ./src/App.ts",
    "doc": "apidoc -i src/ -o static/doc/",
    "eslint": "eslint --ext .ts,.js ./",
    "lock": "npx del-cli --force node_modules && npm i --package-lock-only && npx pnpm i",
    "prepublishOnly": "npm run prerelease",
    "prerelease": "npm test && npm run build",
    "release": "standard-version",
    "start": "npm run build && cross-env NODE_ENV=production node ./dist/App.js",
    "test": "npm run eslint && npm run build && jest --passWithNoTests --detectOpenHandles"
  },
  "engines": {
    "node": ">14.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ],
    "watch": [
      "src"
    ],
    "execMap": {
      "ts": "ts-node"
    },
    "legacyWatch": true,
    "delay": "1000"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@commitlint/cli": "^19.x.x",
    "@commitlint/config-conventional": "^19.x.x",
    "@types/jest": "^29.x.x",
    "@types/koa": "^2.x.x",
    "@types/koa__router": "^12.x.x",
    "@types/node": "^22.x.x",
    "@types/supertest": "^6.x.x",
    "@types/validator": "^13.x.x",
    "@typescript-eslint/eslint-plugin": "^8.x.x",
    "@typescript-eslint/parser": "^8.x.x",
    "apidoc": "^1.x.x",
    "apidoc-plugin-class": "^0.0.4",
    "conventional-changelog-cli": "^5.x.x",
    "copyfiles": "^2.x.x",
    "cross-env": "^7.x.x",
    "del-cli": "^6.x.x",
    "eslint": "^8.x.x",
    "eslint-plugin-jest": "^28.x.x",
    "husky": "^4.x.x",
    "jest": "^29.x.x",
    "jest-html-reporters": "^3.x.x",
    "nodemon": "^3.x.x",
    "standard-version": "^9.x.x",
    "supertest": "^7.x.x",
    "ts-jest": "^29.x.x",
    "ts-node": "^10.x.x",
    "typescript": "^5.x.x"
  },
  "dependencies": {
    "koatty": "~3.11.9",
    "koatty_cacheable": "~1.6.0",
    "koatty_schedule": "~1.6.2",
    "koatty_static": "~1.3.0",
    "koatty_store": "~1.7.0",
    "koatty_validation": "~1.3.6",
    "koatty_views": "~1.1.1",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1"
  }
}